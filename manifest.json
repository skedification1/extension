{
    "name": "Daddyk",
    "description": "Daddyk crypto checker",
    "version": "0.2.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/get_started16.png",
            "32": "/images/get_started32.png",
            "48": "/images/get_started48.png",
            "120": "/images/get_started128.png"
          }
        
    },
    "icons": {
        "16": "/images/get_started16.png",
        "32": "/images/get_started32.png",
        "48": "/images/get_started48.png",
        "120": "/images/get_started128.png"
      },
    "content_scripts": [
      { 
        "matches": ["<all_urls>"],
        "js": ["popup.js"],
        "css": ["style.css"]
    }
    ],
    "permissions": [
        "storage"
    ]
}




