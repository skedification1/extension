{
    "name": "Daddyk checker",
    "description": "Daddyk crypto checker",
    "version": "0.1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
      { 
        "matches": ["<all_urls>"],
        "js": ["popup.js"],
        "css": ["style.css"]
    }
    ],
    "permissions": [
        "storage"
    ]
}